{
  "name": "lochmueller/tagger",
  "type": "typo3-cms-extension",
  "description": "Complex but easy to use tag system with extbase / fluid base and different output possibilities",
  "autoload": {
    "psr-4": {
      "HDNET\\Tagger\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HDNET\\Tagger\\Tests\\": "Tests"
    }
  },
  "keywords": [
    "TYPO3 CMS",
    "Tagger",
    "Extbase"
  ],
  "authors": [
    {
      "name": "Tim LochmÃ¼ller",
      "email": "webmaster@fruit-lab.de",
      "role": "Developer",
      "homepage": "https://github.com/lochmueller"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "typo3/cms-core": "~7.6.0||~8.1.0"
  },
  "replace": {
    "typo3-ter/tagger": "self.version"
  },
  "homepage": "https://github.com/lochmueller/tagger",
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/lochmueller/tagger/issues"
  },
  "require-dev": {
    "namelesscoder/typo3-repository-client": "1.1.x-dev",
    "phpunit/phpunit": "~4.8.0",
    "squizlabs/php_codesniffer": "^2.6",
    "phpmd/phpmd": "^2.4"
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "Package": {
        "partOfMinimalUsableSystem": true
      }
    }
  }
}
